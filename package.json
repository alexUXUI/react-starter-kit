    "babel-loader": "^8.2.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "dotenv-webpack": "^5.1.0",
